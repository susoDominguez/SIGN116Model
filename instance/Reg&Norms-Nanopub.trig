        vocab:strength                "should" ;
        vocab:partOf                  :CIG-DB ;
        vocab:aboutExecutionOf        :ActAdministerPiaglitazone;
        vocab:basedOn                 :CBPiaglitazone .

        :CBPiaglitazone vocab:contribution  "positive" .
}

